<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="corp.skaj.foretagskvitton.activities.UserActivity">

    <fragment
        android:id="@+id/user_bottomnav"
        android:name="corp.skaj.foretagskvitton.view.BottomBarFragment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <Button
        android:id="@+id/reset_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="resetData"
        android:text="ResetData"
        app:layout_constraintBottom_toTopOf="@id/user_bottomnav" />

</android.support.constraint.ConstraintLayout>
